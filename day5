Q1.
#include <stdio.h>
int main(){
    int n;
    scanf("%d",&n);
    printf("%s",(n>=50)?"Pass":"Fail");
    return 0;
}

Q2.                                     #include <stdio.h>
#define PI 3.1416

int main(){
    float length, breadth, side, base, height, radius;
    
    int rectangle ;
    scanf("%f%f", &length, &breadth);
    printf(" %.2f\n", length * breadth);
    
    int square ;
    scanf("%f", &side);
    printf("%.2f", side * side);
    
    int Triangle;
    scanf("%f%f", &base, &height);
    printf("Area of Triangle = %.2f", 0.5 * base * height);
    
    int Circle;
    scanf("%f", &radius);
    printf(" %.2f", PI * radius * radius);
    
    return 0;
}                                              Q3.                                        #include <stdio.h>
#include <math.h>

int main(){
    float x1, y1, x2, y2, distance;
    scanf("%f%f", &x1, &y1);
    scanf("%f%f", &x2, &y2);
    
    distance = sqrt((x2 - x1)*(x2 - x1) + (y2 - y1)*(y2 - y1));
    printf(" %.2f", distance);
    
    return 0;
}                                            Q4:                                              #include <stdio.h>
int main(){
    float P, T, R, SI;
    scanf("%f%f%f", &P, &T, &R);
    SI = (P * T * R) / 100;
    printf(" %.2f", SI);
    return 0;
}                                        