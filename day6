Q1.
	
#include<stdio.h>

int main(){
    int a,b;
    scanf("%d%d",&a,&b);
    if(a>b){
        printf("Max:%d\n",a);
        printf("Min:%d",b);
    }
    else if(b>a){
        printf("Max:%d\n",b);
        printf("Min:%d",a);
    }
    else{
        printf("Equal");
    }
    return 0;
}


Q2.                                      #include<stdio.h>

int main(){
    int a, b, c;
    scanf("%d%d%d", &a, &b, &c);
    if(a > b && a > c)
        printf(" %d", a);
    else if(b > a && b > c)
        printf("%d\n", b);
    else
        printf(" %d\n", c);
    if(a < b && a < c)
        printf("%d", a);
    else if(b < a && b < c)
        printf("%d", b);
    else
        printf("%d", c);

    return 0;
}.                                           Q3.                                            #include<stdio.h>

int main(){
    int num;
    scanf("%d", &num);

    if(num % 2 == 0)
        printf("Even");
    else
        printf("Odd");

    return 0;
}                                               Q4.                                           #include<stdio.h>

int main(){
    int num;
    scanf("%d", &num);

    if(num > 0)
        printf("Positive");
    else if(num < 0)
        printf("Negative");
    else
        printf("Zero");

    return 0;
}                                           Q5.                                         #include<stdio.h>

int main(){
    char ch;
    scanf("%c", &ch);

    if(ch >= 'A' && ch <= 'Z')
        printf("Uppercase Letter");
    else if(ch >= 'a' && ch <= 'z')
        printf("Lowercase Letter");
    else if(ch >= '0' && ch <= '9')
        printf("Digit");
    else
        printf("Special Symbol");

    return 0;
}             